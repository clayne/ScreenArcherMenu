{
    "type": "foldercheckbox",
    "race": true,
    "enter": {
        "type": "local",
        "name": "EnterFolderCheckbox"
    },
    "folder": {
        "format": "full",
        "path": "Data\\F4SE\\Plugins\\SAF\\Adjustments",
        "ext": ".json",
        "pop": false,
        "func": {
            "type": "local",
            "name": "LoadSkeletonAdjustment"
        },
        "reset": {
            "name": "$SAM_Reset",
            "type": "func",
            "func": {
                "type": "sam",
                "name": "ResetSkeletonAdjustment",
                "args": [
                    {
                        "type": "value",
                        "value": true
                    }
                ]
            }
        },
        "load": {
            "type": "toggle",
            "off": "$SAM_Single",
            "on": "$SAM_Multi",
            "var": "folderCheckbox",
            "func": {
                "type": "local",
                "name": "ToggleFolderCheckbox"
            }
        }
    }
}